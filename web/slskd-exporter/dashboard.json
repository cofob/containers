					"uid": "${DS_PROMETHEUS}"
				},
				"refresh": 1,
				"query": {
					"query": "label_values(slskd_transfer_bytes_total, job)",
					"refId": "PromJob"
				},
				"includeAll": true,
				"multi": true,
				"allValue": ".*",
				"current": {
					"selected": true,
					"text": "All",
					"value": ".*"
				}
			},
			{
				"name": "instance",
